{
  "name": "domeo",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:quick": "powershell -ExecutionPolicy Bypass -File scripts/dev-local-quick.ps1",
    "dev:tunnel": "powershell -ExecutionPolicy Bypass -File scripts/start-ssh-tunnel.ps1",
    "dev:start": "powershell -ExecutionPolicy Bypass -File scripts/start-local-dev.ps1",
    "vm:sync": "powershell -ExecutionPolicy Bypass -File scripts/sync-and-run-vm.ps1",
    "vm:sync-only": "powershell -ExecutionPolicy Bypass -File scripts/sync-and-run-vm.ps1 -SyncOnly",
    "build": "npx next build",
    "build:staging": "NODE_ENV=staging next build",
    "build:prod": "NODE_ENV=production next build",
    "build:artifact": "bash scripts/build-artifact.sh",
    "build:artifact:ps1": "powershell -ExecutionPolicy Bypass -File scripts/build-artifact.ps1",
    "start": "next start -H 0.0.0.0 -p 3000",
    "start:staging": "NODE_ENV=staging next start -p 3001",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "postinstall": "prisma generate || true",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "write-sample-excel": "powershell -ExecutionPolicy Bypass -File scripts/write-sample-excel.ps1",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-test-db.ps1",
    "test:e2e:staging": "powershell -ExecutionPolicy Bypass -File scripts/test-with-staging-db.ps1",
    "test:e2e:tunnel": "powershell -ExecutionPolicy Bypass -File scripts/setup-ssh-tunnel.ps1",
    "test:e2e:staging-full": "powershell -ExecutionPolicy Bypass -File scripts/run-e2e-with-staging.ps1",
    "test:predeploy": "powershell -ExecutionPolicy Bypass -File scripts/pre-deploy-check.ps1",
    "test:export": "vitest run lib/export/puppeteer-generator.excel.test.ts",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "cache:clear": "node -e \"require('http').get('http://localhost:3000/api/catalog/doors/complete-data/refresh', r=>{let d='';r.on('data',c=>d+=c);r.on('end',()=>console.log(r.statusCode===200?'Кэш complete-data сброшен':('Ошибка: '+d)));}).on('error',e=>console.error('Сервер не запущен. Запустите npm run dev и повторите.'));\"",
    "export:categories-hierarchy": "tsx scripts/export-categories-hierarchy.ts",
    "export:yml-categories-hierarchy": "tsx scripts/export-yml-categories-hierarchy.ts",
    "compare:yml-csv": "tsx scripts/compare-yml-csv.ts",
    "docs:word": "tsx scripts/build-docx-instructions.ts",
    "seed:catalog-tree": "tsx scripts/seed-catalog-tree.ts",
    "import:final-filled": "tsx scripts/import-final-filled.ts && npm run photos:bind:stable",
    "download:final-filled-photos": "tsx scripts/download-final-filled-photos.ts",
    "download-and-bind:final-filled": "tsx scripts/download-and-bind-photos-from-final-filled.ts",
    "collect:photo-entries": "tsx scripts/collect-photo-entries.ts",
    "bind:final-filled-from-json": "tsx scripts/bind-photos-from-json.ts",
    "build:resolved-from-disk": "tsx scripts/build-resolved-from-disk.ts",
    "fill:handles-excel-colors": "tsx scripts/fill-handles-excel-colors.ts",
    "import:handles-color-description": "tsx scripts/import-handles-color-and-description.ts",
    "photos:bind:stable": "tsx scripts/build-resolved-from-disk.ts && tsx scripts/bind-photos-from-json.ts && tsx scripts/fix-problematic-photo-placeholders.ts && tsx scripts/verify-photo-download-and-binding.ts",
    "db:init": "npm run prisma:generate && npm run prisma:migrate:deploy && npm run prisma:seed",
    "db:reset": "prisma migrate reset --force",
    "clean:data": "tsx scripts/clean-data-before-deploy.ts",
    "deploy:standalone": "powershell -ExecutionPolicy Bypass -File scripts/deploy-standalone-to-vm.ps1",
    "apply:vm-security": "powershell -ExecutionPolicy Bypass -File scripts/apply-vm-security.ps1",
    "deploy:secure": "powershell -ExecutionPolicy Bypass -File scripts/apply-vm-security.ps1 -Deploy",
    "sync:staging": "powershell -ExecutionPolicy Bypass -File scripts/sync-staging-full.ps1",
    "sync:staging:from-sqlite": "npx tsx scripts/sqlite-to-postgres.ts && npm run sync:staging",
    "backup": "echo 'Backup script not implemented yet'",
    "deploy:staging": "chmod +x scripts/deploy-staging.sh && ./scripts/deploy-staging.sh",
    "deploy:prod": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh",
    "dev:safe": "chmod +x dev-safe.sh && ./dev-safe.sh",
    "deploy:staging:safe": "chmod +x deploy-staging-safe.sh && ./deploy-staging-safe.sh",
    "deploy:prod:safe": "chmod +x deploy-production-safe.sh && ./deploy-production-safe.sh",
    "rollback:prod": "chmod +x rollback-production.sh && ./rollback-production.sh",
    "health:staging": "curl -f http://130.193.40.35:3001/api/health",
    "health:prod": "curl -f http://130.193.40.35:3000/api/health",
    "monitor": "chmod +x scripts/monitor-environments.sh && ./scripts/monitor-environments.sh",
    "monitor:ps1": "powershell -ExecutionPolicy Bypass -File scripts/monitor-environments.ps1",
    "git:init": "chmod +x init-git-workflow.sh && ./init-git-workflow.sh",
    "git:feature": "chmod +x create-feature.sh && ./create-feature.sh",
    "git:status": "git status && echo 'Current branch:' && git branch --show-current",
    "new:feature": "chmod +x new-feature.sh && ./new-feature.sh",
    "workflow:status": "echo 'Current branch:' && git branch --show-current && echo 'Remote branches:' && git branch -r",
    "sync:production": "chmod +x sync-from-production.sh && ./sync-from-production.sh",
    "sync:from-staging": "powershell -ExecutionPolicy Bypass -File scripts/sync-from-staging-simple.ps1",
    "sync:from-staging:git": "powershell -ExecutionPolicy Bypass -File scripts/sync-from-staging-git.ps1",
    "import:real-data": "node scripts/import-real-data.js",
    "sync:db-from-vm": "node scripts/sync-db-from-vm.js",
    "sync:data": "chmod +x scripts/sync-data-between-environments.sh && ./scripts/sync-data-between-environments.sh",
    "setup:staging": "chmod +x setup-staging-vm.sh && ./setup-staging-vm.sh",
    "setup:staging:ps1": "powershell -ExecutionPolicy Bypass -File setup-staging-vm.ps1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@fontsource/roboto": "^5.2.8",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-label": "^2.1.7",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "handlebars": "^4.7.8",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.isboolean": "^3.0.3",
    "lodash.isnil": "^4.0.0",
    "lucide-react": "^0.452.0",
    "next": "^15.5.6",
    "next-themes": "^0.2.1",
    "perfect-debounce": "^2.1.0",
    "pkg-types": "^2.3.0",
    "puppeteer-core": "^24.0.0",
    "react": "^19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "sharp": "^0.33.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/handlebars": "^4.0.40",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/winston": "^2.4.4",
    "autoprefixer": "10.4.19",
    "better-sqlite3": "^11.10.0",
    "c12": "^4.0.0-beta.3",
    "docx": "^9.5.3",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "pg": "^8.18.0",
    "postcss": "8.4.39",
    "prisma": "^6.18.0",
    "tailwindcss": "3.4.10",
    "tsx": "^4.7.0",
    "typescript": "5.5.4",
    "vitest": "^2.1.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
